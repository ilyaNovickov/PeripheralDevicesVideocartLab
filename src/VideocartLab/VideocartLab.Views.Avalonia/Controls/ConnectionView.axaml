<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 xmlns:vm="clr-namespace:VideocartLab.ModelVIews;assembly=VideocartLab.ModelVIews"
			 x:DataType="vm:ConnectionModelView"
             x:Class="VideocartLab.Views.Avalonia.ConnectionView">
	<Design.DataContext>
		<vm:ConnectionModelView/>
	</Design.DataContext>
	<Ellipse Fill="Green"
			 PointerPressed="Ellipse_PointerPressed"
			 Width="{Binding Width}"
			 Height="{Binding Height}">
		
	</Ellipse>
</UserControl>
